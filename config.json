{
  "app_name": "NekoDNS",
  "app_executable_name": "NekoDNS",
  "bundle_id": "com.justdev-chris.NekoDNS",
  "version": "1.0.0",
  "build_number": "1",
  "minimum_ios": "15.0",
  "orientation": "portrait",
  
  "architectures": ["arm64"],
  "device_family": ["iphone", "ipad"],
  "package_type": "APPL",
  "development_region": "en",
  
  "supported_orientations": {
    "iphone": ["portrait"],
    "ipad": ["portrait", "landscape"]
  },
  
  "uses_swiftui": true,
  "scene_delegate_class": "$(PRODUCT_MODULE_NAME).SceneDelegate",
  
  "allow_arbitrary_loads": true,
  
  "enable_debug": false,
  "optimization_level": "size"
}
